<h1>All Albums</h1>
  <ul>
    <table>
      <% for (let i = 0; i < albums.length; i++) { %>
        <tr>
          <td id="column-name">
            <li>
              <a href="/albums/<%=albums[i]._id%>"> <%= albums[i].name %> </a>
           </li>
          </td>
          <td id="delete">
              <form action="/albums/<%=albums[i]._id; %>?_method=DELETE" method="POST">
                <input type="submit" value="DELETE"/>
              </form>
          </td>
        </tr>
      <% } %>
    </table>
  </ul> 